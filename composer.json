{
  "name": "nasser/dashboard",
  "description": "A Laravel dashboard package with views and assets.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Nasser",
      "email": "you@example.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "Nasser\\Dashboard\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nasser\\Dashboard\\Providers\\DashboardServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
